{
  "telemetry": {
    "serviceName": "TELEMETRY_SERVICE_NAME",
    "hostname": "TELEMETRY_HOST_NAME",
    "version": "TELEMETRY_SERVICE_VERSION",
    "logger": {
      "level": "LOG_LEVEL",
      "prettyPrint": {
        "__name": "LOG_PRETTY_PRINT_ENABLED",
        "__format": "boolean"
      }
    },
    "tracing": {
      "enabled": {
        "__name": "TELEMETRY_TRACING_ENABLED",
        "__format": "boolean"
      },
      "url": "TELEMETRY_TRACING_URL"
    },
    "metrics": {
      "enabled": "TELEMETRY_METRICS_ENABLED",
      "url": "TELEMETRY_METRICS_URL",
      "interval": "TELEMETRY_METRICS_INTERVAL",
      "buckets": {
        "__name": "TELEMETRY_METRICS_BUCKETS",
        "__format": "json"
      }
    }
  },
  "server": {
    "port": "SERVER_PORT"
  },
  "httpClient": {
    "timeout": {
      "__name": "HTTP_CLIENT_TIMEOUT",
      "__format": "number"
    }
  },
  "osm2pgsql": {
    "slim": {
      "__name": "OSM2PGSQL_SLIM",
      "__format": "boolean"
    },
    "cache": {
      "__name": "OSM2PGSQL_CACHE",
      "__format": "number"
    },
    "processes": {
      "__name": "OSM2PGSQL_PROCESSES",
      "__format": "number"
    },
    "output": "OSM2PGSQL_OUTPUT",
    "generateExpireOutput": {
      "__name": "OSM2PGSQL_GENERATE_EXPIRE_OUTPUT",
      "__format": "boolean"
    },
    "schema": "OSM2PGSQL_SCHEMA",
    "middleSchema": "OSM2PGSQL_MIDDLE_SCHEMA",
    "logger": {
      "level": "OSM2PGSQL_LOG_LEVEL",
      "progress": {
        "__name": "OSM2PGSQL_LOG_PROGRESS",
        "__format": "boolean"
      },
      "sql": {
        "__name": "OSM2PGSQL_LOG_SQL",
        "__format": "boolean"
      },
      "sqlData": {
        "__name": "OSM2PGSQL_LOG_SQL_DATA",
        "__format": "boolean"
      }
    }
  },
  "osmium": {
    "verbose": {
      "__name": "OSMIUM_VERBOSE",
      "__format": "boolean"
    },
    "progress": {
      "__name": "OSMIUM_PROGRESS",
      "__format": "boolean"
    }
  },
  "pgboss": {
    "host": "PGBOSS_HOST",
    "port": {
      "__name": "PGBOSS_PORT",
      "__format": "number"
    },
    "user": "PGBOSS_USERNAME",
    "password": "PGBOSS_PASSWORD",
    "schema": "PGBOSS_SCHEMA",
    "enableSslAuth": {
      "__name": "PGBOSS_ENABLE_SSL_AUTH",
      "__format": "boolean"
    },
    "sslPaths": {
      "ca": "PGBOSS_CA_PATH",
      "key": "PGBOSS_KEY_PATH",
      "cert": "PGBOSS_CERT_PATH"
    },
    "database": "PGBOSS_DATABASE"
  },
  "arstotzka": {
    "enabled": {
      "__name": "ARSTOTZKA_ENABLED",
      "__format": "boolean"
    },
    "serviceId": "ARSTOTZKA_SERVICE_ID",
    "mediator": {
      "timeout": {
        "__name": "MEDIATOR_TIMEOUT",
        "__format": "number"
      },
      "enableRetryStrategy": {
        "__name": "MEDIATOR_ENABLE_RETRY_STRATEGY",
        "__format": "boolean"
      },
      "retryStrategy": {
        "retries": {
          "__name": "MEDIATOR_RETRY_STRATEGY_RETRIES",
          "__format": "number"
        },
        "shouldResetTimeout": {
          "__name": "MEDIATOR_RETRY_STRATEGY_SHOULD_RESET_TIMEOUT",
          "__format": "boolean"
        },
        "isExponential": {
          "__name": "MEDIATOR_RETRY_STRATEGY_IS_EXPONENTIAL",
          "__format": "boolean"
        },
        "delay": {
          "__name": "MEDIATOR_RETRY_STRATEGY_DELAY",
          "__format": "number"
        }
      },
      "actiony": {
        "url": "MEDIATOR_ACTIONY_URL"
      },
      "locky": {
        "url": "MEDIATOR_LOCKY_URL"
      }
    }
  }
}
