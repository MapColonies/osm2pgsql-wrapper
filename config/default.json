{
  "telemetry": {
    "logger": {
      "level": "info",
      "prettyPrint": false
    },
    "tracing": {
      "enabled": false,
      "url": "http://localhost:4318/v1/trace"
    }
  },
  "httpClient": {
    "timeout": 1000
  },
  "osm2pgsql": {
    "slim": true,
    "cache": 2500,
    "processes": 2,
    "output": "flex",
    "generateExpireOutput": true,
    "schema": "public",
    "middleSchema": "public",
    "logger": {
      "level": "info",
      "progress": true,
      "sql": false,
      "sqlData": false
    }
  },
  "osmium": {
    "verbose": false,
    "progress": false
  },
  "arstotzka": {
    "enabled": false,
    "serviceId": "serviceId",
    "mediator": {
      "timeout": 10000,
      "enableRetryStrategy": false,
      "actiony": {
        "url": "http://actiony.com"
      },
      "locky": {
        "url": "http://locky.com"
      }
    }
  }
}
